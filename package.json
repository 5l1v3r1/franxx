{
  "name": "franxx",
  "version": "0.0.0",
  "description": "A vanilla JavaScript router that works everywhere.",
  "repository": {
    "url": "egoist/franxx",
    "type": "git"
  },
  "main": "dist/franxx.cjs.js",
  "module": "dist/franxx.es.js",
  "cdn": "dist/franxx.js",
  "unpkg": "dist/franxx.js",
  "jsdelivr": "dist/franxx.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && npm run test:e2e",
    "lint": "xo",
    "prepublishOnly": "npm run build",
    "build": "bili --format umd,es,cjs,umd-min --inline --buble.exclude ''",
    "example": "node examples/server.js",
    "test:e2e": "run-p --race example cy:run",
    "cy:run": "cypress run"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "bili": "^1.3.1",
    "cypress": "^1.4.2",
    "eslint-config-rem": "^3.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.5",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem",
    "plugins": [
        "cypress"
      ],
    "envs": ["cypress/globals", "browser"],
    "ignores": ["examples/**"],
    "rules": {
      "unicorn/filename-case": "off",
      "import/no-unassigned-import": "off"
    }
  },
  "dependencies": {
    "matchit": "^1.0.1"
  }
}
